{
  "parser" : {
    "type" : "json.stream"
  },
  "timestampSpec": {
    "column":"X", "format":"yyyy-MM-dd", "missingValue": "1992-01-01"
  },
  "dimensionsSpec": {
    "dimensions": ["user_cid", "user_tid", "birthdate", "gender"]
  },
  "metricsSpec":[
    {"name": "email", "typeName": "string"},
    {"name": "ci_profile.xdr_category", "typeName": "map(string,float)"},
    {
      "name": "ci_profile",
      "typeExpr": {
        "base": "boolean",
        "life_style": "string",
        "segment": "string",
        "shopping_channel": "string",
        "family": {
          "fmly_memb_cnt": "int",
          "hhld_memb_cnt": "int",
          "child_y": "boolean",
          "adult_child_y": "boolean",
          "married": "boolean"
        }
      }
    },
    {
      "name": "adot_usage",
      "typeExpr": {
        "life_cycle": "string",
        "stickness": {
          "day_7": "float",
          "day_30": "float"
        }
      }
    },
    {
      "name": "adot_usage.quest",
      "typeExpr": {
        "cube": {
          "received": "int",
          "retention": "int",
          "received_percentile": "int"
        },
        "cone": {
          "received": "int",
          "retention": "int",
          "received_percentile": "int"
        }
      }
    },
    {
      "name": "adot_usage.residence_time",
      "typeExpr": {
        "sumary": {
          "daily_avg_time_sec": "long",
          "percentile": "int"
        }
      }
    }
  ],
  "queryGran": "NONE",
  "segmentGran": "YEAR",
  "fixedSchema": true
}
